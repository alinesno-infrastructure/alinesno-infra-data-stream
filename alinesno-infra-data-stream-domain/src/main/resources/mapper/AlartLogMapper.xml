<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alinesno.infra.data.flink.mapper.AlartLogMapper">

    <resultMap type="com.alinesno.infra.data.flink.entity.AlartLogEntity" id="AlartLogResult">
                    <result property="jobConfigId" column="job_config_id"/>
                    <result property="jobName" column="job_name"/>
                    <result property="message" column="message"/>
                    <result property="type" column="type"/>
                    <result property="status" column="status"/>
                    <result property="failLog" column="fail_log"/>
            </resultMap>

    <update id="deleteByJobConfigId">
      delete  from alart_log
      where job_config_id = #{jobConfigId}
    </update>

</mapper>
